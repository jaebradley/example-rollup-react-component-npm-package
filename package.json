{
  "name": "@jaebradley/example-rollup-react-component-npm-package",
  "version": "0.0.0-development",
  "description": "Example React Component for npm Publication using Rollup",
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "files": [
    "./build"
  ],
  "scripts": {
    "build": "rollup -c",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy-storybook-ci": "storybook-to-ghpages --ci",
    "gc": "commit",
    "lint": "eslint --ext .js,.jsx .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "start": "rollup -c -w",
    "storybook": "start-storybook -p 6006",
    "test": "jest --coverage --passWithNoTests",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/example-rollup-react-component-npm-package.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rollup",
    "react",
    "react component",
    "rollup react",
    "component",
    "example react component",
    "example rollup react component"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/example-rollup-react-component-npm-package/issues"
  },
  "homepage": "https://github.com/jaebradley/example-rollup-react-component-npm-package#readme",
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/prompt": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "@storybook/addon-a11y": "^3.4.1",
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-centered": "^3.4.1",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.1",
    "@storybook/addon-options": "^3.4.1",
    "@storybook/channels": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "@storybook/storybook-deployer": "^2.3.0",
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
